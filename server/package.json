{
  "name": "server",
  "description": "Projet de base pour le cours de LOG2990",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "tsc && node out/server/app/www.js",
    "lint": "tslint --project tsconfig.json \"./app/**/*.ts\" --format stylish",
    "test": "mocha",
    "coverage": "nyc mocha",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "rimraf out node_modules",
    "postinstall": "rimraf false"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "include": [
      "app/services/**/*.ts",
      "app/utils/**/*.ts",
      "app/model/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/I*.ts",
      "**/*.spec.ts",
      "app/services/interfaces.ts",
      "app/services/socket/socketCallback.ts",
      "app/model/game/game.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true,
    "cache": true,
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@meanie/mongoose-to-json": "^2.3.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/mongodb": "^3.1.19",
    "@types/mongoose": "^5.3.10",
    "@types/request": "^2.48.1",
    "@types/socket.io-client": "^1.4.32",
    "@types/sprintf-js": "^1.1.1",
    "@types/uuid": "^3.4.4",
    "aws-sdk": "^2.403.0",
    "axios": "^0.18.0",
    "bitmap-ts": "^1.0.3",
    "body-parser": "^1.18.3",
    "chai-bytes": "^0.1.2",
    "cookie-parser": "1.4.1",
    "cors": "2.8.4",
    "dateformat": "^3.0.3",
    "debug": "^4.1.1",
    "express": "~4.16.0",
    "file-saver": "^2.0.0",
    "files": "^0.8.2",
    "inversify": "^4.13.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemon": "^1.18.9",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.10",
    "request": "^2.88.0",
    "serve-favicon": "~2.5.0",
    "socket.io": "^2.4.0",
    "socket.io-client": "^2.2.0",
    "socket.io-mock": "^1.2.3",
    "sprintf-js": "^1.1.2",
    "ts-node": "^7.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/chai": "~4.1.4",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.4",
    "@types/del": "^3.0.1",
    "@types/express": "4.16.0",
    "@types/express-serve-static-core": "4.16.0",
    "@types/file-saver": "^2.0.0",
    "@types/mocha": "5.2.5",
    "@types/morgan": "~1.7.35",
    "@types/multer": "^1.3.7",
    "@types/node": "~10.12.18",
    "@types/node-uuid": "0.0.28",
    "@types/sinon": "~7.0.3",
    "@types/sinon-express-mock": "~1.3.7",
    "@types/socket.io": "^1.4.38",
    "@types/valid-url": "^1.0.2",
    "chai": "~4.1.0",
    "concurrently": "^4.0.1",
    "del": "^3.0.0",
    "mocha": "~5.2.0",
    "nyc": "^13.1.0",
    "rimraf": "2.6.2",
    "sinon": "~7.2.2",
    "sinon-express-mock": "~2.1.0",
    "ts-mockito": "^2.3.1",
    "tslint": "~5.11.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-microsoft-contrib": "5.2.0",
    "typescript": "~3.1.0",
    "valid-url": "^1.0.9"
  }
}
